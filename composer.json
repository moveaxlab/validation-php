{
    "name": "elevenlab/validation-php",
    "description": "Validation package based on Illuminate Validation",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Valerio Cervo",
            "email": "valerio.cervo@moveax.it"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.4",
        "propaganistas/laravel-phone": "^4.1",
        "illuminate/validation": "5.2.*",
        "illuminate/translation": "5.2.*",
        "nesbot/carbon": "~1.20"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "symfony/var-dumper": "^4.2",
        "chainside/validation-testvectors": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "ElevenLab\\Validation\\": "src/",
            "ElevenLab\\Validation\\Tests\\": "tests/"
        }
    },
    "autoload-dev": {
      "classmap": [
          "tests/TestCase.php"
      ]
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "chainside/validation-testvectors",
                "version": "dev-master",
                "source": {
                    "url": "git@bitbucket.org:chainside/validation-test-vectors.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ]
}
