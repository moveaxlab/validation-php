{
    "name": "moveaxlab/validation-php",
    "description": "Validation package based on Illuminate Validation",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Valerio Cervo",
            "email": "valerio.cervo@moveax.it"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.4",
        "illuminate/validation": "5.2.*",
        "illuminate/translation": "5.2.*",
        "nesbot/carbon": "~1.20",
        "propaganistas/laravel-phone": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "symfony/var-dumper": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "ElevenLab\\Validation\\": "src/",
            "ElevenLab\\Validation\\Tests\\": "tests/"
        }
    },
    "autoload-dev": {
      "classmap": [
          "tests/TestCase.php"
      ]
    }
}
